pipeline{
	agent any
	stages{
		stage ('complie stage'){
			step{
				withMaven(maven: '\apache-maven-3.5.4'){
					sh 'mvn clean compile'
				}
			}
		}
		
		stage ('test stage'){
			step{
				withMaven(maven: '\apache-maven-3.5.4'){
					sh 'mvn test'
				}
			}
		}
		
		stage ('package stage'){
			step{
				withMaven(maven: '\apache-maven-3.5.4'){
					sh 'mvn package'
				}
			}
		}				
	}
}